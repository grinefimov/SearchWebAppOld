@model List<SearchWebApp.Models.SearchResult>

@{
  ViewData["Title"] = "Index";
}

<h1>Search</h1>

<div>@Model[0].SearchService</div>

@for (int i = 0; i < Model.Count; i++)
{
<div class="mb-3">
  <div>@(i + 1)</div>
  <div><a href="@Model[i].Url" target="_blank">@Model[i].Title</a></div>
  <div><a href="@Model[i].Url" target="_blank">@Model[i].Url</a></div>
  <div>@Model[i].Snippet</div>
</div>
}
